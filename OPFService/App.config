<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/></startup>
	<appSettings>

		<add key="DatabaseConnectionString" value=""/>
		<add key="DomainNameForQueries" value="softeam.local"/>
		
		<add key="OPFClientRecognitionKeyword" value="STXPWDCHECK_PW_OPF"/>

		<add key="UsePwnedPwd" value="true"/>
		<add key="PwnedPwdDatabaseTableName" value="Hashes"/>

		<add key="UseForbiddenList" value="true"/>
