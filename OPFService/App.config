		<add key="MatchesDatabaseTableName" value="Matches"/>
		<add key="ContMatchesDatabaseTableName" value="ContMatches"/>
		<add key="MatchFile" value="D:\repos\OpenPasswordFilter_SQL\Lists\opfmatch.txt"/>
		<add key="ContMatchFile" value="D:\repos\OpenPasswordFilter_SQL\Lists\opfcont.txt"/>
		
		<add key="LoggingEnabled" value="true"/>
		<add key="LogEventSource" value="PasswordFilter"/>
		<add key="LogFilePath" value="C:\GitHub\OpenPasswordFilter\Log\opfservice.log"/>


		<add key="PwdMinLength" value="12"/>
		<add key="PwdMaxSameCharRepetition" value="2"/>
		<add key="PwdNotIncludingUserData" value="true"/>
